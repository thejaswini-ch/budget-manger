{
    "manifest_version":2,
    "name":"Budget Manager",
    "version": "1.0",
    "description":"This extension tracks your overall spendings.",
    "icons":{
        "128": "icon128.jpg",
        "48": "icon48.jpg",
        "16": "icon16.jpg"
    },
    "browser_action":{
        "default_icon": "icon128.jpg",
        "default_popup": "popup.html"
    },
    "options_page" : "options.html",
    "permissions":[
"storage",
"notifications"
    ]
        
    
}